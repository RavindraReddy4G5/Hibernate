<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
<class name="com.office.Employee" table="employee">
<id name="eid" column="eid"></id>
<property name="ename" column="ename"></property>
<property name="edesignation" column="edesignation"></property>
<property name="ebloodgroup" column="ebloodgroup"></property>
<property name="esalary" column="esalary"></property>
<property name="egender" column="egender"></property>
<property name="ecomname" column="ecomname"></property>
<property name="ephone" column="ephone"></property>
<property name="eage" column="eage"></property>
</class>
</hibernate-mapping>
